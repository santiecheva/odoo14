<?xml version='1.0' encoding='utf-8' ?>
<odoo>
    <record id='asociados_form_view' model='ir.ui.view'>
    <field name='name'> asociados form user </field>
    <field name='model'>modelo.consorcio</field>
    <field name='arch' type='xml'>
        <form string='form asociados'>
            <sheet>
                <group>
                    <field name='name'/>
                    <field name='porcentaje'/>
                    <field name='Porcentaje_pago'/>
                </group>
            </sheet>
        </form>
    </field>
    </record>

    <record id='asociados_tree_view' model='ir.ui.view'>
        <field name='name'> asociados tree user </field>
        <field name='model'>modelo.consorcio</field>
        <field name='arch' type='xml'>
            <tree string='tree asociados'>
                <field name='name'/>
                <field name='porcentaje'/>
                <field name='Porcentaje_pago'/>
            </tree>
        </field>
    </record>

<record id='action_asociados' model= 'ir.actions.act_window'>
    <field name='name'>action.consorcio</field>
    <field name='type'>ir.actions.act_window</field>
    <field name='res_model'>modelo.consorcio</field>
    <field name='view_mode'>tree,form</field>
</record>

</odoo>